    {

        "ASWSEBDockerrunVersion": 2,
        "containerDefinitions": [
            {
                "name": "client",
                "image": "harishmani09/multi-client",
                "hostname": "client",
                "essential": false
            },
            {
                "name": "server",
                "image": "harishmani09/multi-server",
                "hostname": "api",
                "essential": false
            },
            {
                "name": "worker",
                "image": "harishmani09/multi-worker",
                "hostname": "worker",
                "essential": false
            },
            {
                "name": "nginx",
                "image": "harishmani09/multi-nginx",
                "hostname": "nginx",
                "essential": true,
                "portMappings": [
                    {
                        "hostPort": 80,
                        "containerPort": 80
                    }
                ],
                "links": ["client", "server"]

            }
        ]



    }